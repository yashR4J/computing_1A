// EXAM C Standard Library Headers
#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>
#include <math.h>
#include <ctype.h>
#include <string.h>

int main(void) {

	int num = 0, sum = 0;
	
	while (scanf("%d", num) == 1) {
		if (num % 2 != 0) {
			sum = sum + num
		}
	}

	printf("%d", sum);
	
	return 0;
}


