// Question 1: Program returns the sum of all positive, odd numbers 
// that are entered

// EXAM C Standard Library Headers
#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>
#include <math.h>
#include <ctype.h>
#include <string.h>

int main(void) {

	int a = 3, b = 31, c = 10;
	double z = a / b % c;
	
	printf("%f\n\n\n", z);
	
	
	
	return 0;
}


